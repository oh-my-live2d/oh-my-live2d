{
  "name": "oh-my-live2d-root",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "changed": "lerna version --conventional-commits --create-release github",
    "changed:major": "lerna version major --conventional-commits --create-release github",
    "demo": "pnpm -F oh-my-live2d run demo",
    "demo:umd": "pnpm -F oh-my-live2d run demo:umd",
    "dev": "pnpm -F oh-my-live2d run dev",
    "docs:build": "pnpm -r build && typedoc && cpy CHANGELOG.md docs/src && pnpm -r docs:build",
    "docs:dev": "concurrently \"typedoc --watch\" \"pnpm -r docs:dev\"",
    "docs:preview": "pnpm -r docs:preview",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "lint:eslint": "eslint --ext .ts --fix .",
    "lint:prettier": "prettier --check --write .",
    "prepare": "husky",
    "preview:new-version": "lerna version --dry-run",
    "release": "lerna publish from-package --yes",
    "test:vue": "pnpm -F vue-app run dev"
  },
  "nano-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts}": "eslint --fix",
    "package.json": "sort-package-json"
  },
  "devDependencies": {
    "@lerna-lite/cli": "^3.3.3",
    "@lerna-lite/publish": "^3.3.3",
    "@lerna-lite/version": "^3.3.3",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "concurrently": "^8.2.2",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "prettier": "3.2.5",
    "sort-package-json": "^2.8.0",
    "tianjie": "^0.0.12",
    "typedoc": "0.25.13",
    "typedoc-plugin-markdown": "4.0.0-next.60",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.8.0"
  }
}
